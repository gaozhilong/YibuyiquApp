<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0"
	id="yibuyiqu.app" version="0.0.1">
	<name>Yibuyiqu App</name>
	<description>
		A sample Apache Cordova application that responds to the
		deviceready
		event.
	</description>
	<author email="thym-dev@eclipse.org" href="http://www.eclipse.org/thym">
		Eclipse.org - Thym
	</author>

	<content src="index.html" />
	<!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
	<engine name="android" version="4.0.2" />

	<preference name="AndroidPersistentFileLocation" value="Internal" />
	<feature name="File">
		<param name="wp-package" value="File" />
		<param name="id" value="cordova-plugin-file" />
	</feature>
	<feature name="Device">
		<param name="android-package" value="org.apache.cordova.device.Device" />
		<param name="wp-package" value="Device" />
		<param name="id" value="cordova-plugin-device" />
	</feature>
	<feature name="SBD">
		<param name="android-package" value="fr.omegasolutions.plugin.Sbd" />
		<param name="onload" value="true" />
	</feature>
	<feature name="Status Bar Disabler">
		<param name="id" value="cordova-plugin-android-status-bar-disabler" />
	</feature>
</widget>
